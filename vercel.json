{
    "version": 2,
    "functions": {
        "api/**": {
            "runtime": "nodejs20.x"
        }
    },
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  